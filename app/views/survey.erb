<h1><%= @survey.title %></h1>

<% questions = @survey.questions%>
<% logger.info(questions) %>
<ul>
	<% questions.each do |question| %>
		<% choices = question.choices %>
		<% logger.info(choices) %>
		<li><%= question.question %></li>
			<ul>
			<% choices.each do |choice| %>
  				<li><%= choice.choice %></li>
  		</ul>
  		<% end %>
  	</li>
	<% end %>
</ul>

	